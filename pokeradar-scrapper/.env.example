LOG_LEVEL=info

# Fetching tier filter: "super-slow", "slow", "fast", "super-fast", or "all" (default: all)
FETCHING_TIER=all

# MongoDB Configuration (required)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/pokeradar

# Maximum number of retry attempts for a request
MAX_RETRY_ATTEMPTS=1

# Proxy rotation (set PROXY_ENABLED=true to route requests through rotating proxy)
PROXY_ENABLED=false
PROXY_URL=http://username:password@p.webshare.io:80