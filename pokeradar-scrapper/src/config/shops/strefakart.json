{
    "id": "strefakart",
    "name": "strefakart.pl",
    "baseUrl": "https://strefakart.pl",
    "searchUrl": "/wszystkie-produkty/?s=",
    "selectors": {
      "searchPage": {
        "article": {
          "type": "css",
          "value": "li.product"
        },
        "productUrl": {
          "type": "css",
          "value": "li.product a",
          "extract": "href"
        },
        "title": {
          "type": "css",
          "value": "h2"
        }
      },
      "productPage": {
        "title": {
          "type": "css",
          "value": "h1.product_title"
        },
        "price": {
          "type": "css",
          "value": "p.price span",
          "format": "european"
        },
        "available": {
          "type": "css",
          "value": "div.product.instock"
        }
      }
    }
  }
  