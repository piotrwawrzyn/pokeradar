{
  "id": "tcglove",
  "name": "TCG Love",
  "baseUrl": "https://www.tcglove.pl",
  "searchUrl": "/pl/searchquery/{query}/1/full/4",
  "antiBot": {
    "maxConcurrency": 1
  },
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "product-tile"
      },
      "productUrl": {
        "type": "css",
        "value": "a",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "h3.product-tile__name"
      },
      "price": {
        "type": "css",
        "value": "span.price__value",
        "format": "european"
      },
      "unavailable": {
        "type": "text",
        "value": "Powiadom mnie o dostępności"
      },
      "available": {
        "type": "text",
        "value": "Do koszyka"
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "h-tab-panel.js__product-price-gross span.price__value",
        "format": "european"
      },
      "available": {
        "type": "text",
        "value": "Dodaj do koszyka"
      }
    }
  }
}
