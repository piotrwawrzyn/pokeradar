{
  "id": "battlestash",
  "name": "BattleStash",
  "disabled": true,
  "baseUrl": "https://battlestash.pl",
  "engine": "playwright",
  "fetchingTier": "super-slow",
  "searchUrl": "/?post_type=product&s=",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": ["div.products div.product-small", "article"]
      },
      "productUrl": {
        "type": "css",
        "value": ["p.product-title a", "div.entry-image a"],
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": ["p.product-title", "div.entry-summary p"]
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "p.price span",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": "div.product-main button[name='add-to-cart']"
      }
    }
  }
}
