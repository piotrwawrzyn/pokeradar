{
  "id": "poketrader",
  "name": "Poketrader",
  "baseUrl": "https://poketrader.eu",
  "searchUrl": "/szukaj.html/szukaj=",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "div.Okno"
      },
      "productUrl": {
        "type": "css",
        "value": "h3 a",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "h3"
      },
      "price": {
        "type": "css",
        "value": ["span.Cena", "span.CenaPromocyjna"],
        "extract": "ownText"
      },
      "available": {
        "type": "text",
        "value": "Do koszyka"
      },
      "unavailable": {
        "type": "css",
        "value": ".ProduktBezZakupu",
        "matchSelf": true
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "#KartaProduktu #InformacjeProduktu span[itemprop='price']",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": "#PrzyciskKupowania:not([style*=\"display:none\"]) span.DoKoszykaKartaProduktu"
      }
    }
  }
}
