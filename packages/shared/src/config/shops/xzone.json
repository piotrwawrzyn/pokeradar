{
  "id": "xzone",
  "name": "XZone",
  "baseUrl": "https://www.xzone.pl",
  "searchUrl": "/katalog.php?term=",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "div.product-item"
      },
      "productUrl": {
        "type": "css",
        "value": "div.product-item-name a",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "div.product-item-name"
      },
      "price": {
        "type": "css",
        "value": "span.price",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": ".expedice-date .green_text"
      },
      "unavailable": {
        "type": "css",
        "value": ".expedice-date .red_text, .expedice-date .orange_text"
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "span.price",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": "div.white-block-product ul.dostupnost_holder li.green_text"
      }
    }
  }
}
