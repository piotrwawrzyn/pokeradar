{
  "id": "przyczolek",
  "name": "Przyczółek",
  "baseUrl": "https://przyczolek.pl",
  "searchUrl": "/pl/searchquery/",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "div.products div.product"
      },
      "productUrl": {
        "type": "css",
        "value": "a.prodname",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "span.productname"
      },
      "price": {
        "type": "css",
        "value": "div.price em",
        "extract": "ownText"
      },
      "available": {
        "type": "text",
        "value": "Do koszyka"
      },
      "unavailable": {
        "type": "text",
        "value": "Powiadom mnie o dostępności"
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "div.price__container em.main-price",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": "fieldset.addtobasket-container:not(.none)"
      }
    }
  }
}
