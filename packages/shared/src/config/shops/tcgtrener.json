{
  "id": "tcgtrener",
  "name": "TCG Trener",
  "baseUrl": "https://tcgtrener.pl",
  "searchUrl": "/pl/searchquery/",
  "antiBot": {
    "useProxy": true
  },
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "product-list product-tile"
      },
      "productUrl": {
        "type": "css",
        "value": "product-link a",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "h3.product-tile__name"
      },
      "price": {
        "type": "css",
        "value": "span.price__value",
        "format": "european"
      },
      "unavailable": [
        {
          "type": "text",
          "value": "tymczasowo niedostępny"
        },
        {
          "type": "text",
          "value": "spodziewana dostawa"
        }
      ],
      "available": [
        {
          "type": "text",
          "value": "na wyczerpaniu"
        },
        {
          "type": "text",
          "value": "duża ilość"
        },
        {
          "type": "text",
          "value": "średnia ilość"
        }
      ]
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "h-tab-panel.js__product-price-gross span.price__value",
        "format": "european"
      },
      "available": {
        "type": "text",
        "value": "Dodaj do koszyka"
      }
    }
  }
}
