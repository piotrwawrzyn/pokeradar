{
  "id": "zgrani",
  "name": "Zgrani.pl",
  "baseUrl": "https://zgrani.pl",
  "searchUrl": "/pl/search?text={query}",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "div.product"
      },
      "productUrl": {
        "type": "css",
        "value": "a.product__name",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "a.product__name"
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "h2#projector_price_value",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": "button.projector_buy__button:not([disabled])"
      }
    }
  }
}
