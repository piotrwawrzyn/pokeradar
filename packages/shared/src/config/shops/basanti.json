{
  "id": "basanti",
  "name": "Basanti",
  "baseUrl": "https://gryplanszowe-basanti.pl",
  "searchUrl": "/en/search?controller=search&orderby=position&orderway=desc&search_query=",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "div.item-product-list"
      },
      "productUrl": {
        "type": "css",
        "value": "div.product_name a",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "div.product_name"
      },
      "unavailable": {
        "type": "text",
        "value": "Oczekiwanie na dostawÄ™"
      },
      "available": {
        "type": "css",
        "value": "button[data-button-action=\"add-to-cart\"]:not([disabled])"
      },
      "price": {
        "type": "css",
        "value": "span.price",
        "format": "us"
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "span[itemprop=\"price\"]",
        "format": "us"
      },
      "available": {
        "type": "css",
        "value": "div.add button:not([disabled])"
      }
    }
  }
}
