{
  "id": "hejmistrzu",
  "name": "Hej Mistrzu",
  "baseUrl": "https://hejmistrzu.pl/",
  "searchUrl": "?s={query}&post_type=product",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "li.entry"
      },
      "productUrl": {
        "type": "css",
        "value": "h2 a",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "h2 a"
      },
      "unavailable": {
        "type": "css",
        "value": ".outofstock",
        "matchSelf": true
      },
      "price": {
        "type": "css",
        "value": "span.woocommerce-Price-amount bdi",
        "format": "european"
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "span.woocommerce-Price-amount bdi",
        "format": "european"
      },
      "available": [
        {
          "type": "css",
          "value": "div.summary form.cart:not(.variations_form) button.single_add_to_cart_button"
        },
        {
          "type": "json-attribute",
          "value": "form[data-product_variations]",
          "attribute": "data-product_variations",
          "jsonFilter": "is_in_stock",
          "condition": "some"
        }
      ]
    }
  }
}
