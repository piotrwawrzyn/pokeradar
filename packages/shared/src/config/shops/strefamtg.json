{
  "id": "strefamtg",
  "name": "StrefaMTG",
  "baseUrl": "https://strefamtg.pl",
  "searchUrl": "/szukaj?controller=search&s=",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "article"
      },
      "productUrl": {
        "type": "css",
        "value": "h2.product-miniature__title a",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "h2.product-miniature__title a"
      },
      "price": {
        "type": "css",
        "value": "span.price",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": "button[data-button-action='add-to-cart']:not([disabled])"
      },
      "unavailable": {
        "type": "css",
        "value": "button[data-button-action='add-to-cart'][disabled]"
      }
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "div.current-price span",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": "button.add-to-cart:not([disabled])"
      }
    }
  }
}
