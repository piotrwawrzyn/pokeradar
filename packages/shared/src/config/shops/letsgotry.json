{
  "id": "letsgotry",
  "name": "LetsGoTry",
  "baseUrl": "https://letsgotry.pl",
  "searchUrl": "/szukaj?controller=search&s={query}&order=product.price.desc",
  "selectors": {
    "searchPage": {
      "article": {
        "type": "css",
        "value": "article"
      },
      "productUrl": {
        "type": "css",
        "value": "h3.product-title a",
        "extract": "href"
      },
      "title": {
        "type": "css",
        "value": "h3.product-title span"
      },
      "price": {
        "type": "css",
        "value": "span.price"
      },
      "available": {
        "type": "css",
        "value": "button.add_to_cart:not([disabled])"
      },
      "unavailable": [
        {
          "type": "css",
          "value": "button.add_to_cart[disabled]"
        },
        {
          "type": "text",
          "value": "Brak"
        }
      ]
    },
    "productPage": {
      "price": {
        "type": "css",
        "value": "div.current-price span",
        "format": "european"
      },
      "available": {
        "type": "css",
        "value": "button.add-to-cart:not([disabled])"
      }
    }
  }
}
